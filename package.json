    "type": "git",
    "url": "git://github.com/bem-site/mds-wrapper.git"
  },
  "author": "Andrey Kuznetsov <andrey.kuznetsov48@yandex.ua> (http://github.com/tormozz48)",
  "maintainers": [
    "Andrey Kuznetsov <andrey.kuznetsov48@yandex.ua> (http://github.com/tormozz48)"
  ],
  "contributors": [
    "Andrey Kuznetsov <andrey.kuznetsov48@yandex.ua> (http://github.com/tormozz48)",
    "Nikolay Ilchenko <tavria@yandex-team.ru> (http://github.com/tavriaforever)"
  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "dependencies": {
    "lodash": "~2.4.1",
    "request": "2.51.x",
    "vow": "~0.4.1"
  },
  "devDependencies": {
    "mocha": "~2.1.0",
    "mocha-lcov-reporter": "0.0.1",
    "should": "~4.6.x",
    "istanbul": "^0.3.5",
    "jshint": "~2.5.11",
    "jscs": "~1.10.x"
  },
  "main": "index.js",
  "scripts": {
    "mocha": "node_modules/.bin/mocha",
    "istanbul": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "codestyle": "node_modules/.bin/jshint . && node_modules/.bin/jscs -c .jscs.js .",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "coveralls": "npm i coveralls && npm run cover && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test": "npm run codestyle && npm run cover"
  },
  "readmeFilename": "README.md",
