  ],
  "engines": {
    "node": ">= 0.8.0"
  },
  "dependencies": {
    "request": "^2.53.x",
    "vow": "^0.4.x"
  },
  "devDependencies": {
    "mocha": "~2.1.x",
    "mocha-lcov-reporter": "0.0.x",
    "should": "~4.6.x",
    "istanbul": "^0.3.x",
    "jshint": "~2.6.x",
    "jscs": "^1.11.x"
  },
  "main": "index.js",
  "scripts": {
    "mocha": "node_modules/.bin/mocha",
    "istanbul": "istanbul cover ./node_modules/mocha/bin/_mocha",
    "codestyle": "node_modules/.bin/jshint . && node_modules/.bin/jscs -c .jscs.js .",
    "cover": "istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec",
    "coveralls": "npm install coveralls && npm run cover && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js && rm -rf ./coverage",
    "test": "npm run codestyle && npm run coveralls"
  },
